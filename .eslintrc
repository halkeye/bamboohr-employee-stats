{
  "extends": [ "semistandard", "eslint:recommended" ],
  "rules": {
    "no-console": 0,
    "no-unused-vars": [2],
    "no-var": 2,
    "no-invalid-this": 2,
    "eqeqeq": 2,
    "handle-callback-err": 2,
    "consistent-return": 2,
    "comma-dangle": 2
  },
  "env": {
    "browser": true,
    "es6": true
  },
}
